<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义倒计时按钮-->
    <declare-styleable name="CountdownButton">
        <!--倒计时进行中的按钮文字，要传入stringformat格式，如%s秒-->
        <attr name="tickText" format="string" />
        <!--倒计时完成后的按钮文字，默认为“重新发送”-->
        <attr name="finishedText" format="string"/>
        <!--倒计时总时间，默认为60s-->
        <attr name="millisInFuture" format="integer"/>
        <!--倒计时的间隔，默认为1s-->
        <attr name="countDownInterval" format="integer"/>
    </declare-styleable>


</resources>